                                    Titulo="Prestamos por Provedor" >
                            <i:Interaction.Behaviors>
                                <comportamientos:Seguridad LlaveSeguridad="Reporte - Prestamos por Proveedor" TipoSeguridadPropiedad="Visible"/>
                            </i:Interaction.Behaviors>
                        </misControlesWpf:MenuOpcionAButon>
                        <misControlesWpf:MenuOpcionAButon
                                    Background="White"
                                    BorderBrush="#FFC5C5C5"
                                    ColorBarra="{DynamicResource AccentColorBrush}"
                                    ColorFondo="{DynamicResource AccentColorBrush}"
                                    ColorImagen="{DynamicResource AccentColorBrush}"
                                    ColorTitulo="#FF565755"
                                    Command="{Binding ComandoMostrarBoletasPoProveedor}"
                                    Imagen="M15,25.010004L19,25.010004C19.552,25.010004 20,25.458004 20,26.010004 20,26.562004 19.552,27.010004 19,27.010004L15,27.010004C14.448,27.010004 14,26.562004 14,26.010004 14,25.458004 14.448,25.010004 15,25.010004z M6,22L6,26 10,26 10,22z M15,21.010004L19,21.010004C19.552,21.010004 20,21.458004 20,22.010004 20,22.562004 19.552,23.010004 19,23.010004L15,23.010004C14.448,23.010004 14,22.562004 14,22.010004 14,21.458004 14.448,21.010004 15,21.010004z M4,20L12,20 12,28 4,28z M15,14.010004L19,14.010004C19.552,14.010004 20,14.458004 20,15.010004 20,15.562004 19.552,16.010004 19,16.010004L15,16.010004C14.448,16.010004 14,15.562004 14,15.010004 14,14.458004 14.448,14.010004 15,14.010004z M6,11L6,15 10,15 10,11z M15,10.010004L19,10.010004C19.552,10.010004 20,10.458004 20,11.010004 20,11.562004 19.552,12.010004 19,12.010004L15,12.010004C14.448,12.010004 14,11.562004 14,11.010004 14,10.458004 14.448,10.010004 15,10.010004z M4,9L12,9 12,17 4,17z M18,2.4139996L18,6.0309906 21.616991,6.0309906z M2,2L2,30 22,30 22,8.0309887 16,8.0309887 16,2z M2,0L18.414,0 24,5.5860004 24,30C24,31.103,23.103001,32,22,32L2,32C0.89700007,32,0,31.103,0,30L0,2C0,0.89699936,0.89700007,0,2,0z"
                                    Titulo="Boletas por Provedor" >
                            <i:Interaction.Behaviors>
                                <comportamientos:Seguridad LlaveSeguridad="Reporte - Boletas por Proveedor" TipoSeguridadPropiedad="Visible"/>
                            </i:Interaction.Behaviors>
                        </misControlesWpf:MenuOpcionAButon>
                        <misControlesWpf:MenuOpcionAButon
                                    Background="White"
                                    BorderBrush="#FFC5C5C5"
                                    ColorBarra="{DynamicResource AccentColorBrush}"
                                    ColorFondo="{DynamicResource AccentColorBrush}"
                                    ColorImagen="{DynamicResource AccentColorBrush}"
