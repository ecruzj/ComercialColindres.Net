<clases:MetroWindowExt
    x:Class="ComercialColindres.Windows.ReportesWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:clases="clr-namespace:WPFCore.UtilidadesWindows;assembly=WPFCore"
    xmlns:controls="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:misControlesWpf="clr-namespace:MisControlesWPF;assembly=MisControlesWPF"
    xmlns:controlsMahApps="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"
    xmlns:Comportamientos="clr-namespace:WPFCore.Comportamientos;assembly=WPFCore"
    xmlns:HelpersMahApps="http://metro.mahapps.com/winfx/xaml/controls"
    xmlns:syncfusion="http://schemas.syncfusion.com/wpf"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:comportamientos="clr-namespace:ComercialColindres.Comportamientos"
    xmlns:wpfCoreConvertidores="clr-namespace:WPFCore.Convertidores;assembly=WPFCore"
    Title="{Binding TituloPantalla}"                              
    BorderBrush="{DynamicResource AccentColorBrush}"
    BorderThickness="1"
    d:DesignHeight="1224"
    d:DesignWidth="1024"
    MostrarDialogo="{Binding DialogSettings, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
    SaveWindowPosition="True"
    ShowFlyout="{Binding SlidePanelIndice, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
    WindowStartupLocation="CenterScreen"    
    mc:Ignorable="d">
    <clases:MetroWindowExt.Resources>
        <wpfCoreConvertidores:BoolToStringConverter x:Key="BoolToStringConverter" />
        <wpfCoreConvertidores:DateTimeToShortString x:Key="DateTimeToShortString" />
        <wpfCoreConvertidores:StateValueToColorConverter x:Key="StateValueToColorConverter" />
        <wpfCoreConvertidores:BinaryImageConverter x:Key="BinaryImageConverter" />
        <wpfCoreConvertidores:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter" />
    </clases:MetroWindowExt.Resources>
    
    <clases:MetroWindowExt.DataContext>
        <Binding Path="ReportesViewModel" Source="{StaticResource Locator}" />
    </clases:MetroWindowExt.DataContext>

    <clases:MetroWindowExt.Flyouts>
        <controls:FlyoutsControl>
            <controls:Flyout x:Name="CierresBoletaFlyout"
                Width="655"
                Margin="100,0,0,0"
                Header="Pago Masivo de Boletas"
                IsModal="True"
                Position="Right"
                Theme="Adapt">
                <Grid Margin="10">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>
                    <TabControl Grid.Row="0">
                        <TabItem HelpersMahApps:ControlsHelper.HeaderFontSize="16" Header="Cierre Masivo">
                            <Grid Margin="5">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>
                                <Grid>
                                    <GroupBox Header="PAGO MASIVO DE BOLETAS" Grid.Row="0">
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="Auto" />
                                            </Grid.RowDefinitions>
                                            <Grid Grid.Row="0">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition />
                                                    <ColumnDefinition />
                                                    <ColumnDefinition />
                                                    <ColumnDefinition Width="40"/>
                                                </Grid.ColumnDefinitions>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="Auto" MinHeight="30" />
                                                    <RowDefinition Height="Auto" MinHeight="30" />
                                                </Grid.RowDefinitions>
                                                <misControlesWpf:HeaderTextBlock Grid.Column="0" Text="Proveedor" />
                                                <misControlesWpf:BodyTextBlock Grid.Row="1" Grid.Column="0" 
                                                                Text="{Binding BoletaPaymentPending.VendorName, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                                                <misControlesWpf:HeaderTextBlock
                                        Grid.Row="0"
                                        Grid.Column="1"
                                        Text="Monto Total" />
                                                <misControlesWpf:BodyTextBlock Grid.Row="1" Grid.Column="1" 
                                                                FontSize="12"
                                                                FontWeight="Bold"
                                                                Text="{Binding BoletaPaymentPending.SaldoPendiente,
                                                                Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, StringFormat=N2}" />
                                                <misControlesWpf:HeaderTextBlock
                                        Grid.Row="0"
                                        Grid.Column="2"
                                        Text="Monto Justificado" />
                                                <misControlesWpf:BodyTextBlock Grid.Row="1" Grid.Column="2" 
                                                                FontSize="12"
                                                                FontWeight="Bold"
                                                                Text="{Binding DatoBoletaCierre.CantidadJustificada,
                                                                Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, StringFormat=N2}" />
                                            </Grid>
                                        </Grid>
                                    </GroupBox>
                                </Grid>
                                <Grid Grid.Row="1">
                                    <GroupBox Header="DETALLE DE PAGO DE BOLETA">
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="Auto" />
                                                <RowDefinition Height="*" />
                                            </Grid.RowDefinitions>
                                            <Grid Grid.Row="0">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width= "*" />
                                                    <ColumnDefinition Width="*" />
                                                    <ColumnDefinition Width="*" />
                                                    <ColumnDefinition Width="*" />
                                                    <ColumnDefinition Width="40"/>
                                                </Grid.ColumnDefinitions>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="Auto" MinHeight="30" />
                                                    <RowDefinition Height="Auto" MinHeight="30" />
                                                    <RowDefinition Height="Auto" MinHeight="30" />
                                                    <RowDefinition Height="Auto" MinHeight="30" />
                                                </Grid.RowDefinitions>
                                                <misControlesWpf:HeaderTextBlock Grid.Column="0" Text="Forma de Pago" 
                                                              FontWeight="SemiBold"
                                                              TextAlignment="Center"/>
                                                <ComboBox
                                        Margin="5"
                                        Grid.Row="1"
                                        Grid.Column="0"                                        
                                        ItemsSource="{Binding ListaFormaPagos}"
                                        DisplayMemberPath="Descripcion"
                                        SelectedItem="{Binding FormaDePagoSeleccionada, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                        SelectedValue="{Binding DatoBoletaCierre.FormaDePago, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" 
                                        SelectedValuePath="Descripcion"/>
                                                <misControlesWpf:HeaderTextBlock Grid.Column="1" Text="Banco" 
                                                              FontWeight="SemiBold"
                                                              TextAlignment="Center"/>
                                                <ComboBox
                                        Margin="5"
                                        Grid.Row="1"
                                        Grid.Column="1"
                                        ItemsSource="{Binding ListadoBancos}"
                                        DisplayMemberPath="Descripcion"                                        
                                        SelectedItem="{Binding BancoSeleccionado, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                        SelectedValue="{Binding DatoBoletaCierre.BancoId, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" 
                                        SelectedValuePath="BancoId"
                                        IsEnabled="{Binding FormaDePagoSeleccionada.RequiereBanco,Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                                                <misControlesWpf:HeaderTextBlock Grid.Column="2" Grid.ColumnSpan="2" Text="Linea Crédito" 
                                                              FontWeight="SemiBold"
                                                              TextAlignment="Center"/>
                                                <ComboBox Margin="5"
                                              Grid.Row="1"
                                              Grid.Column="2"
                                              Grid.ColumnSpan="2"
                                              HorizontalContentAlignment="Center" 
                                              ItemsSource="{Binding LineasCredito}"
                                              SelectedItem="{Binding LineaCreditoSeleccionada, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                              SelectedValue="{Binding DatoBoletaCierre.LineaCreditoId, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                              SelectedValuePath="LineaCreditoId">
                                                    <ComboBox.ItemTemplate>
                                                        <DataTemplate>
                                                            <Grid>
                                                                <Grid.ColumnDefinitions>
                                                                    <ColumnDefinition Width="Auto"/>
                                                                    <ColumnDefinition Width="*"/>
                                                                </Grid.ColumnDefinitions>
                                                                <TextBlock Margin="5" Grid.Column="0" Text="{Binding CodigoLineaCredito}"/>
                                                                <TextBlock Margin="5" Grid.Column="1" Text="{Binding CuentaFinanciera}"/>
                                                            </Grid>
                                                        </DataTemplate>
                                                    </ComboBox.ItemTemplate>
                                                </ComboBox>
                                                <misControlesWpf:HeaderTextBlock Grid.Column="0" Grid.Row="2" Text="Fecha Pago" TextAlignment="Center"
                                                              FontWeight="SemiBold"/>
                                                <DatePicker Margin="5" Grid.Column="0" Grid.Row="3" SelectedDate="{Binding DatoBoletaCierre.FechaPago, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                                                <misControlesWpf:HeaderTextBlock Grid.Column="1" Grid.Row="2" Text="Documento Ref" TextAlignment="Center"
                                                              FontWeight="SemiBold"/>
                                                <misControlesWpf:InputTextBox Grid.Row="3" Grid.Column="1" 
                                                               Text="{Binding DatoBoletaCierre.NoDocumento, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                               MaxLength="50"
                                                               IsEnabled="{Binding FormaDePagoSeleccionada.RequiereBanco,Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                                                <misControlesWpf:HeaderTextBlock Text="Monto"
                                                              Grid.Column="2" 
                                                              Grid.Row="2" 
                                                              Grid.ColumnSpan="2" 
                                                              TextAlignment="Center" 
                                                              FontWeight="SemiBold"/>
                                                <misControlesWpf:InputTextBox Grid.Row="3" Grid.Column="2" Grid.ColumnSpan="2"
                                                               FontSize="12"
                                                               FontWeight="Bold"
                                                               Text="{Binding DatoBoletaCierre.Monto, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, StringFormat=N2}"
                                                               Comportamientos:TextBoxMaskBehavior.MaximumValue="18"/>
                                                <misControlesWpf:OnlyImageAutoSizeButton
                                        Grid.Row="2"
                                        Grid.Column="4"
                                        Width="21"
                                        Height="21"
                                        Margin="5"
                                        HorizontalAlignment="Left"
                                        Background="{DynamicResource AccentColorBrush}"
                                        Command="{Binding ComandoAgregarPagoBoleta}"
                                        Imagen="{DynamicResource AgregarPathImagen}" />
                                                <misControlesWpf:OnlyImageAutoSizeButton
                                        Grid.Row="3"
                                        Grid.Column="4"
                                        Width="21"
                                        Height="21"
                                        Margin="5"
                                        HorizontalAlignment="Left"
                                        Background="{DynamicResource AccentColorBrush}"
                                        Command="{Binding CommandHoleTotalToPayment}"
                                        Imagen="{DynamicResource FullPathImagen}" />
                                            </Grid>
                                            <Grid Grid.Row="1">
                                                <DataGrid                                    
                                Margin="5"
                                AutoGenerateColumns="False"
                                CanUserAddRows="False"
                                CanUserDeleteRows="False"
                                IsReadOnly="True"                                        
                                ItemsSource="{Binding ListaBoletaCierresModel, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                SelectedItem="{Binding BoletaCierreSeleccionado, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">
                                                    <DataGrid.Columns>
                                                        <DataGridTemplateColumn Width="55">
                                                            <DataGridTemplateColumn.CellTemplate>
                                                                <DataTemplate>
                                                                    <StackPanel Orientation="Horizontal">
                                                                        <misControlesWpf:OnlyImageAutoSizeButton
                                                        Width="21"
                                                        Height="21"
                                                        Margin="2"
                                                        Background="{DynamicResource AccentColorBrush}"
                                                        Command="{Binding ReportesViewModel.ComandoRemoverPagoBoleta, Source={StaticResource Locator}}"
                                                        Imagen="{DynamicResource QuitarPathImagen}" />
                                                                    </StackPanel>
                                                                </DataTemplate>
                                                            </DataGridTemplateColumn.CellTemplate>
                                                        </DataGridTemplateColumn>
                                                        <DataGridTextColumn Binding="{Binding CodigoLineaCredito}" Header="Linea Crédito" />
                                                        <DataGridTextColumn Binding="{Binding FormaDePago}" Header="Forma Pago" />
                                                        <DataGridTextColumn Binding="{Binding Monto, StringFormat=N2}" Header="Monto" />
                                                        <DataGridTextColumn Binding="{Binding NombreBanco}" Header="Banco" />
                                                        <DataGridTextColumn Binding="{Binding NoDocumento}" Header="Documento Ref" />
                                                        <DataGridTextColumn Binding="{Binding FechaPago, StringFormat=d}" Header="FechaPago" />
                                                    </DataGrid.Columns>
                                                </DataGrid>
                                            </Grid>
                                        </Grid>
                                    </GroupBox>
                                </Grid>
                                <misControlesWpf:AppBarPanel Grid.Row="2" Background="{DynamicResource AccentColorBrush}">
                                    <misControlesWpf:AppBarButton
                            Command="{Binding ComandoGuardarPagosBoleta}"
                            Imagen="{DynamicResource GuardarPathImagen}"
                            Titulo="Guardar" />
                                </misControlesWpf:AppBarPanel>
                            </Grid>
                        </TabItem>
                        <TabItem HelpersMahApps:ControlsHelper.HeaderFontSize="16" Header="Validaciones" 
                                 Visibility="{Binding BoletasWithError, Converter={StaticResource BoolToVisibilityConverter}}" >
                            <Grid Margin="5">
                                <DataGrid
                                        Grid.Row="1"
                                        Margin="5"
                                        AutoGenerateColumns="False"
                                        CanUserAddRows="False"
                                        CanUserDeleteRows="False"
                                        IsReadOnly="True"
                                        ItemsSource="{Binding BoletaPendingsWithError, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Binding="{Binding CodigoBoleta}" Header="Boleta" />
                                        <DataGridTextColumn Binding="{Binding MensajeError}" Header="Mensaje Validacion" Foreground="Red" />
                                    </DataGrid.Columns>
                                </DataGrid>
                            </Grid>
                        </TabItem>
                    </TabControl>
                    <Border Grid.RowSpan="3" Visibility="{Binding MostrarVentanaEspera}">
                        <Grid>
                            <misControlesWpf:BloqueoPanel/>
                            <controlsMahApps:ProgressRing IsActive="True" IsLarge="False"/>
                        </Grid>
                    </Border>
                </Grid>
            </controls:Flyout>
        </controls:FlyoutsControl>
    </clases:MetroWindowExt.Flyouts>
    
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="10" />
            <RowDefinition Height="35" />
            <RowDefinition Height="33*" />
        </Grid.RowDefinitions>
        <Grid Grid.Row="1">
            <Rectangle
                Height="5"
                VerticalAlignment="Top"
                Fill="{DynamicResource AccentColorBrush}" />
            <Rectangle Margin="0,9,0,0" Fill="{DynamicResource AccentColorBrush}" />
            <misControlesWpf:HeaderTextBlock
                Height="20"
                Margin="30,9.5,0,5.5"
                Foreground="White"
                FontSize="16"
                Text="MENU" />
            <StackPanel
                Height="60"
                Margin="0,-16.286,30,-8.714"
                HorizontalAlignment="Right"
                Orientation="Horizontal">
                <misControlesWpf:TextAppBarButton
                    Background="{DynamicResource AccentColorBrush}"
                    Command="{Binding CommandShowBoletaMasiveClose}"
                    Imagen="{Binding Mode=OneWay, Source={StaticResource CloseMasivePathImagen}}"
                    Titulo="Cierre Masivo"
                    ToolTip="{StaticResource RefrescarToolTip}"
                    Visibility="{Binding IsCierreMasivo, Converter={StaticResource BoolToVisibilityConverter}}"/>
                <misControlesWpf:TextAppBarButton
                    Background="{DynamicResource AccentColorBrush}"
                    Command="{Binding ComandoRefrescar}"
                    Imagen="{Binding Mode=OneWay, Source={StaticResource RefrescarPathImagen}}"
                    Titulo="REFRESCAR"
                    ToolTip="{StaticResource RefrescarToolTip}" />                
            </StackPanel>
        </Grid>
        <Grid Grid.Row="2" Margin="5">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="235" />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>
            <Grid Background="{DynamicResource AccentColorBrush}">
                <ScrollViewer
                    HorizontalContentAlignment="Stretch"
                    VerticalContentAlignment="Stretch"
                    VerticalScrollBarVisibility="Auto">
                    <WrapPanel>
                        <misControlesWpf:MenuOpcionAButon
                                    Background="White"
                                    BorderBrush="#FFC5C5C5"
